{
    "name": "mssqltask-app",
    "version": "0.0.1",
    "description": "Tasks for many Microsoft SQL Servers",
    "main": "dist/src/index.js",
    "bin": "dist/src/index.js",
    "scripts": {
        "test": "node ./dist/test/index.js",
        "update": "npx npm-check-updates -u && npm i && npm audit fix && node ./node_modules/vv-template-nodebackend/index.js",
        "build": "tsc",
        "compile_win": "tsc && npm rebuild && node ./.auto/compile.win.before.js && pkg . --t node14-win-x64 --out-path compile-win --compress GZip &&  node ./.auto/compile.win.after.js",
        "compile_lin": "tsc && npm rebuild && node ./.auto/compile.lin.before.js && pkg . --t node14-linux-arm64 --out-path compile-lin --compress GZip &&  node ./.auto/compile.lin.after.js",
        "precommit": "node ./.auto/precommit.js"
    },
    "pkg": {
        "node_version": "14.17.6",
        "scripts": "dist/src/*.js",
        "assets": [
            "dist/src/package.json",
            "node_modules/vv-logger/dist/src/*.js",
            "node_modules/vv-logger/dist/src/**/.js",
            "node_modules/backdepot/dist/src/*.js",
            "node_modules/backdepot/dist/src/**/*.js",
            "node_modules/mssqltask/dist/src/*.js",
            "node_modules/mssqltask/dist/src/**/.js",
            "node_modules/better-sqlite3/build/Release/better_sqlite3.node"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/VasilevVitalii/mssqltask-app.git"
    },
    "author": "Vitalii Vasilev",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/VasilevVitalii/mssqltask-app/issues"
    },
    "homepage": "https://github.com/VasilevVitalii/mssqltask-app#readme",
    "devDependencies": {
        "@types/node": "16.11.7",
        "@typescript-eslint/eslint-plugin": "5.3.1",
        "@typescript-eslint/parser": "5.3.1",
        "adm-zip": "^0.5.9",
        "eslint": "8.2.0",
        "npm-check-updates": "^12.0.2",
        "pkg": "^5.4.1",
        "prettier": "2.4.1",
        "resedit": "^1.2.0",
        "ts-node": "10.4.0",
        "typescript": "4.4.4",
        "vv-template-nodebackend": "^0.0.14"
    },
    "dependencies": {
        "backdepot": "^0.0.8",
        "mssqltask": "^0.0.10",
        "vv-common": "^0.0.7",
        "vv-logger": "^0.0.4"
    }
}
